{
  "author": "Niall Bourke",
  "cite": "L. Wang, Z. Wu, L. Chen, Y. Sun, W. Lin, G. Li, iBEAT V2. 0: a multisite-applicable, deep learning-based pipeline for infant cerebral cortical surface reconstruction, Nature Protocol, vol. 18(5), 2023, pp. 1488-1509",
  "command": "/opt/gear-env/bin/python /flywheel/v0/run.py",
  "config": {
    "age": {
      "default": null,
      "description": "Age of the infant at the time of scan (in months). If set to 0, the age will be taken from the subject metadata.",
      "maximum": 24,
      "minimum": 1
    }
  },
  "custom": {
    "analysis-level": "participant",
    "flywheel": {
      "classification": {
        "components": [],
        "keywords": [
          "super-resolution",
          "volume",
          "template"
        ],
        "modality": [
          "MR"
        ],
        "type": [
          "nifti"
        ]
      },
      "suite": "UNITY Apps"
    },
    "gear-builder": {
      "category": "analysis",
      "image": "flywheel/ibeat2:0.1.2"
    },
    "kcl-app-binary": "app/main.sh",
    "kcl-app-modalities": [
      "anat"
    ],
    "license": {
      "dependencies": [],
      "main": {
        "name": "",
        "url": ""
      },
      "non-commercial-use-only": true
    }
  },
  "description": "Flywheel gear for iBEAT V2.0 - a deep learning-based pipeline for infant cerebral cortical surface reconstruction. Must include 'gpu' in tag to run. Expects T2w input, T1w optional. ",
  "environment": {
    "FLYWHEEL": "/flywheel/v0",
    "LANG": "en_US.UTF-8",
    "PWD": "/InfantData",
    "OS": "Linux",
    "LIBRARY_PATH": "/usr/local/cuda/lib64/stubs",
    "NV_CUDA_COMPAT_PACKAGE": "cuda-compat-11-4",
    "NV_LIBCUBLAS_VERSION": "11.6.5.2-1",
    "LANGUAGE": "en_US:en",
    "LD_LIBRARY_PATH": "/usr/lib/fsl/5.0:/usr/local/InfantPipeline/lib/itk/lib:/usr/local/InfantPipeline/lib/caffe/lib:/usr/local/nvidia/lib:/usr/local/nvidia/lib64:/usr/local/InfantPipeline/bin/skel/lib",
    "NV_LIBNCCL_PACKAGE_VERSION": "2.11.4-1",
    "NV_LIBCUBLAS_DEV_VERSION": "11.6.5.2-1",
    "NV_CUDNN_PACKAGE_NAME": "libcudnn8",
    "NV_LIBNCCL_DEV_PACKAGE_VERSION": "2.11.4-1",
    "NV_LIBNPP_PACKAGE": "libnpp-11-4=11.4.0.110-1",
    "FSLDIR": "/usr/share/fsl/5.0",
    "CUDA_VERSION": "11.4.3",
    "NV_CUDNN_PACKAGE": "libcudnn8=8.2.4.15-1+cuda11.4",
    "NV_LIBCUBLAS_PACKAGE_NAME": "libcublas-11-4",
    "FSLMULTIFILEQUIT": "TRUE",
    "NVIDIA_REQUIRE_CUDA": "cuda>=11.4 brand=tesla,driver>=418,driver<419 brand=tesla,driver>=440,driver<441 brand=tesla,driver>=450,driver<451 brand=tesla,driver>=460,driver<461",
    "NVIDIA_DRIVER_CAPABILITIES": "compute,utility",
    "NV_CUDA_LIB_VERSION": "11.4.3-1",
    "NV_LIBCUSPARSE_VERSION": "11.6.0.120-1",
    "NV_LIBNCCL_PACKAGE_NAME": "libnccl2",
    "NV_NVML_DEV_VERSION": "11.4.120-1",
    "NV_LIBNPP_DEV_PACKAGE": "libnpp-dev-11-4=11.4.0.110-1",
    "NV_CUDA_CUDART_VERSION": "11.4.148-1",
    "NV_CUDNN_PACKAGE_DEV": "libcudnn8-dev=8.2.4.15-1+cuda11.4",
    "PATH": "/usr/local/InfantPipeline/lib/caffe/bin:/usr/local/InfantPipeline/lib/caffe/python:/usr/local/InfantPipeline/bin:/usr/local/InfantPipeline/bin/mni/bin:/usr/lib/fsl/5.0:/usr/local/InfantPipeline/bin/commands-links:/usr/local/nvidia/bin:/usr/local/cuda/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
    "NVARCH": "x86_64",
    "NV_LIBCUBLAS_PACKAGE": "libcublas-11-4=11.6.5.2-1",
    "NV_LIBCUBLAS_DEV_PACKAGE_NAME": "libcublas-dev-11-4",
    "PERL5LIB": "/usr/local/InfantPipeline/bin/mni/lib/perl5/5.8.5",
    "NV_LIBNCCL_PACKAGE": "libnccl2=2.11.4-1+cuda11.4",
    "NV_LIBCUSPARSE_DEV_VERSION": "11.6.0.120-1",
    "NV_LIBNCCL_DEV_PACKAGE_NAME": "libnccl-dev",
    "FSLOUTPUTTYPE": "NIFTI_GZ",
    "NV_CUDA_CUDART_DEV_VERSION": "11.4.148-1",
    "PYCAFFE_ROOT": "/usr/local/InfantPipeline/lib/caffe/python",
    "NV_LIBCUBLAS_DEV_PACKAGE": "libcublas-dev-11-4=11.6.5.2-1",
    "NV_LIBNCCL_DEV_PACKAGE": "libnccl-dev=2.11.4-1+cuda11.4",
    "NV_NVTX_VERSION": "11.4.120-1",
    "NV_LIBNPP_VERSION": "11.4.0.110-1",
    "NV_CUDNN_VERSION": "8.2.4.15",
    "LC_ALL": "en_US.UTF-8",
    "PYTHONPATH": "/usr/local/InfantPipeline/lib/caffe/python",
    "NVIDIA_VISIBLE_DEVICES": "all",
    "NCCL_VERSION": "2.11.4-1",
    "NV_LIBNPP_DEV_VERSION": "11.4.0.110-1",
    "CAFFE_ROOT": "/usr/local/InfantPipeline/lib/caffe"
  },
  "inputs": {
    "api-key": {
      "base": "api-key",
      "read-only": true
    },
    "input": {
      "base": "file",
      "description": "T2w input file (usually axial or isotropic reconstruction)",
      "optional": false
    },
    "t1": {
      "base": "file",
      "description": "T1w input file",
      "optional": true
    }
  },
  "label": "ibeat2",
  "license": "MIT",
  "maintainer": "Niall Bourke <niall.bourke@kcl.ac.uk>",
  "name": "ibeat2",
  "source": "https://github.com/iBEAT-V2/iBEAT-V2.0-Docker",
  "url": "",
  "version": "0.1.2"
}